<div class="container mt-5">
  <h1>Your movie lists</h1>

  <div class="cards">
      <% @lists.each do |list| %>
        <div class="card-product">
        <h2><i class="fas fa-film"></i> <%= list.name %></h2>
        </div>
      <% end %>
  </div>
  <%= link_to "Make a new list", new_list_path %>
</div>
